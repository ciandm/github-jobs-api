styled.js",[],"/Users/cian/Documents/Coding/frontend-mentor/github-jobs-api/src/theme/typography.js",[],"/Users/cian/Documents/Coding/frontend-mentor/github-jobs-api/src/hooks/useFetchJobs.js",["120"],"import { useEffect, useReducer } from 'react'\nimport axios from 'axios';\n\nconst ACTIONS = {\n  NEW_REQUEST: 'new-request',\n  HAS_LOAD_MORE: 'has-load-more',\n  GET_DATA: 'get-data',\n  ERROR: 'error',\n  LOAD_MORE: 'load-more'\n}\n\nfunction reducer(state, action) {\n  switch (action.type) {\n    case ACTIONS.NEW_REQUEST:\n      return {\n        loading: true,\n        jobs: []\n      }\n    case ACTIONS.HAS_LOAD_MORE:\n      return {\n        ...state,\n        hasLoadMore: action.payload.hasLoadMore\n      }\n    case ACTIONS.GET_DATA:\n      return {\n        ...state,\n        loading: false,\n        jobs: action.payload.jobs\n      }\n    case ACTIONS.ERROR:\n      return {\n        ...state,\n        loading: false,\n        error: action.payload.error,\n        jobs: []\n      }\n    default:\n      return state;\n  }\n}\n\nconst API_URL = 'https://jobs.github.com/positions.json';\n\nconst useFetchJobs = (params, page) => {\n  const [state, dispatch] = useReducer(reducer, { jobs: [], loading: true, hasLoadMore: false });\n\n\n  // fetch jobs\n  useEffect(() => {\n    const cancelToken1 = axios.CancelToken.source();\n    dispatch({ type: ACTIONS.NEW_REQUEST });\n    axios.get(API_URL, {\n      cancelToken: cancelToken1.token,\n      params: {\n        ...params,\n        page\n      }\n    })\n      .then((res) => {\n        dispatch({ type: ACTIONS.GET_DATA, payload: { jobs: res.data } });\n      })\n      .catch(error => {\n        if (axios.isCancel) return;\n        dispatch({ type: ACTIONS.ERROR, payload: error })\n      })\n\n    const cancelToken2 = axios.CancelToken.source();\n    axios.get(API_URL, {\n      cancelToken: cancelToken2.token,\n      params: {\n        ...params,\n        page: page + 1\n      }\n    })\n      .then((res) => {\n        dispatch({ type: ACTIONS.HAS_LOAD_MORE, payload: { hasLoadMore: res.data.length !== 0 } });\n      })\n      .catch(error => {\n        if (axios.isCancel) return;\n        dispatch({ type: ACTIONS.ERROR, payload: error })\n      })\n\n    return () => {\n      cancelToken1.cancel();\n      cancelToken2.cancel();\n    }\n  }, [params, page])\n\n  return state;\n}\n\nexport default useFetchJobs;","/Users/cian/Documents/Coding/frontend-mentor/github-jobs-api/src/components/Form/Input/Input.jsx",[],"/Users/cian/Documents/Coding/frontend-mentor/github-jobs-api/src/components/Form/Checkbox/Checkbox.jsx",[],"/Users/cian/Documents/Coding/frontend-mentor/github-jobs-api/src/components/Form/Input/Input.styled.js",[],"/Users/cian/Documents/Coding/frontend-mentor/github-jobs-api/src/components/Form/Checkbox/Checkbox.styled.js",[],"/Users/cian/Documents/Coding/frontend-mentor/github-jobs-api/src/components/JobBoard/JobCard/JobCard.jsx",[],"/Users/cian/Documents/Coding/frontend-mentor/github-jobs-api/src/components/JobBoard/JobCard/JobCard.styled.js",[],"/Users/cian/Documents/Coding/frontend-mentor/github-jobs-api/src/components/JobDetail/JobDetail.js",[],"/Users/cian/Documents/Coding/frontend-mentor/github-jobs-api/src/components/JobDetail/JobDetail.styled.js",[],"/Users/cian/Documents/Coding/frontend-mentor/github-jobs-api/src/components/JobBoard/JobCard/JobCardSkeleton/JobCardSkeleton.jsx",[],"/Users/cian/Documents/Coding/frontend-mentor/github-jobs-api/src/components/JobBoard/JobCard/JobCardSkeleton/JobCardSkeleton.styled.js",[],{"ruleId":"121","replacedBy":"122"},{"ruleId":"123","replacedBy":"124"},{"ruleId":"125","severity":1,"message":"126","line":1,"column":27,"nodeType":"127","messageId":"128","endLine":1,"endColumn":36},{"ruleId":"125","severity":1,"message":"129","line":28,"column":5,"nodeType":"127","messageId":"128","endLine":28,"endColumn":10},{"ruleId":"125","severity":1,"message":"130","line":1,"column":10,"nodeType":"127","messageId":"128","endLine":1,"endColu